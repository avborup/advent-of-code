prev, pos = 50, 50;
part1, part2 = 0, 0;

for dir, val in input().find_all(r/(R|L)(\d+)/n) {
  match dir {
    "R" => pos += val,
    "L" => pos -= val,
  };

  pos %= 100;
  pos += 100 if pos < 0;
  part2 += val // 100;

  if pos == 0 {
    part1 += 1;
    part2 += 1;
  } else if (prev != 0) {
    part2 += 1 if dir == "R" and prev > pos;
    part2 += 1 if dir == "L" and prev < pos;
  };

  prev = pos;
};

print("Part 1:", part1);
print("Part 2:", part2);
