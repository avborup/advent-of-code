prev, pos = 50, 50;
parts = 0, 0;

for dir, amount in input().find_all(r/(R|L)(\d+)/n) {
  match dir {
    "R" => pos += amount,
    "L" => pos -= amount,
  };

  pos = ((pos % 100) + 100) % 100;
  parts += (0, amount // 100);

  match pos {
    0 => parts += (1, 1),
    _ => if prev != 0 {
      parts += (0, 1) if dir == "R" and prev > pos;
      parts += (0, 1) if dir == "L" and prev < pos;
    },
  };

  prev = pos;
};

print("Part 1:", parts[0]);
print("Part 2:", parts[1]);
